<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/dp4" />
            <stroke android:width="@dimen/dp1" android:color="@color/divider_line" />
            <solid android:color="@color/page_bg" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/dp4" />
            <stroke android:width="@dimen/dp1" android:color="@color/color_999999" />
        </shape>
    </item>
</selector>